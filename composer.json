{
    "name": "questpass/questpass-php-sdk",
    "description": "Questpass PHP integration toolkit",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^5.6 || ^7.1"
    },
    "autoload": {
        "psr-4": {
            "Questpass\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Questpass\\SDK\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "suggest": {
        "ext-curl": "Needed to support CurlHttpClient implementation",
        "ext-json": "Needed to provide Subscribers login via OAuth"
    },
    "require": {
        "php": "^5.3.2 || ^7.0",
        "kub-at/php-simple-html-dom-parser": "^1.7"
    }
}
